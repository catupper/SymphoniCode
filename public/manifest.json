{
  "manifest_version": 2,
  "name": "SymphoniCode",
  "short_name": "SymphoniCode",
  "version": "0.0.1",
  "description": "Symphonic Orchestra drived by your code.",
  "content_scripts": [
    {
      "matches": ["https://atcoder.jp/*/custom_test*"],
      "js": ["js/symphony.js"]
    }
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [ "webNavigation", "https://atcoder.jp/*/custom_test*" ]
}
